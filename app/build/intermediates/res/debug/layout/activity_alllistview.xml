<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffff">

    <ImageView
        android:id="@+id/main_img"
        android:layout_width="108dp"
        android:layout_height="83dp"
        android:scaleType="centerCrop" />

    <co.kr.app.bangbanggokgok.common.H5TextView
        android:id="@+id/area"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_toRightOf="@+id/main_img"
        android:singleLine="true"
        android:text="테스트"
        android:textColor="#898989"
        android:textSize="12sp" />

    <co.kr.app.bangbanggokgok.common.H5TextView
        android:id="@+id/payType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/area"
        android:layout_below="@+id/area"
        android:layout_marginTop="2dp"
        android:text="원룸 월세"
        android:textColor="#000000"
        android:textSize="15dp"
        android:textStyle="bold" />


    <co.kr.app.bangbanggokgok.common.H5TextView
        android:id="@+id/deposit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/payType"
        android:layout_marginLeft="4dp"
        android:layout_toRightOf="@+id/payType"
        android:text="100/70"
        android:textColor="#75c703"
        android:textSize="17dp"
        android:textStyle="bold" />

    <co.kr.app.bangbanggokgok.common.H5TextView
        android:id="@+id/one_report"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/payType"
        android:layout_below="@+id/payType"
        android:layout_marginTop="2dp"
        android:singleLine="true"
        android:text="초근접 역세권! 깔끔한 신축 복층형 원룸"
        android:textColor="#000000"
        android:textSize="10sp" />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/one_report"
        android:layout_below="@+id/one_report"
        android:layout_marginTop="2dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/option_01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/icon_list_00_01"
            android:layout_marginRight="2dp"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/option_02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/icon_list_00_02"
            android:layout_marginRight="2dp"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/option_03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/icon_list_00_03"
            android:layout_marginRight="2dp"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/option_04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/icon_list_00_04"
            android:layout_marginRight="2dp"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/option_05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/icon_list_00_05"
            android:visibility="gone" />


    </LinearLayout>


    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/main_img"
        android:background="#c8cfd6" />
</RelativeLayout><!-- From: file:/home/johnny/AndroidStudioProjects/App.BangBangGokGok/app/src/main/res/layout/activity_alllistview.xml -->